{
    "name": "descom/aws-sns-notification",
    "description": "Package to Laravel to received notification from AWS SNS",
    "license": "MIT",
    "authors": [
        {
            "name": "AwsSnsNotification Support",
            "email": "soporte@descom.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "aws/aws-php-sns-message-validator": "^1.1",
        "illuminate/config": "^9.0|^10.0",
        "illuminate/http": "^9.0|^10.0",
        "illuminate/routing": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "descom/dev": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.4",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.5|^8.0",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Descom\\AwsSnsNotification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Descom\\AwsSnsNotification\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwsSnsNotification\\Skeleton\\SkeletonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
